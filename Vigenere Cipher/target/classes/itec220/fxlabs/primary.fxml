            <Label BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fitHeight="114.0" fitWidth="200.0">
                     <image>
                        <Image url="@ascii%20table.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
         </center>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
